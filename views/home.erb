<body id="home_body">
	<div id="blog_logo">
		<img id="blog_image" src="/images/my_blog_logo.png" alt="">
	</div>
	<div id="post_container">
		<% @users.each do |user| %>
				<% user.posts.each do |post| %>
					<div class="user">
						<%= user.name %> 
						<p><%= post.created_at %></p>
						<p><%= post.title %></p>
						<p><%= post.content %></p>
					</div>
			<% end %>
		<% end %>
	</div>
</body>